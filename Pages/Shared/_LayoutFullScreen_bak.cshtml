<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"] - emPower</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">

    <environment include="Development">
        <link href="~/assets/css/LoginSheet.css" rel="stylesheet" nonce="@Context.Items["CSP-Nonce"]" />
    </environment>
    <environment exclude="Development">
        <link href="~/assets/css/LoginSheet.css" rel="stylesheet" nonce="@Context.Items["CSP-Nonce"]" />
    </environment>
    @RenderSection("Headers", required: false)
    <!-- ICONS -->
    <link rel="icon" type="image/x-icon" sizes="96x96" href="/favicon.ico">
</head>

<!-- BODY -->
<body class="app flex-row align-items-center">

    <div class="container">
        <div class="row justify-content-center">
            @RenderBody()
        </div>
    </div>




</body>
</html>
